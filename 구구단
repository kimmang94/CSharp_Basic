using System;

namespace ConsoleApp5
{
    class Program
    {
        static void Main(string[] args)
        {
            for (int i = 2; i <= 9; i++)
            {
                Console.WriteLine($"{i}ë‹¨");
                for (int j = 1; j <= 9; j++)
                {
                    Console.WriteLine($"{i}x{j}={i*j}");
                }
            }
        }
    }
}
